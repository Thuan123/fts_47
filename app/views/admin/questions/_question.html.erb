<tr>
  <td><%= question.id %></td>
  <td><%= question.content %></td>
  <td><%= question.category.name %></td>
  <td><%= question.question_type %></td>
  <td>
    <% question.answers.each do |answer| %>
      <li>
        <%= answer.content %>
        |<%= answer.correct %>
      </li>
    <% end %>
  </td>
  <td>
    <%= link_to t("question.edit"), "#" %>
    <%= link_to t("question.delete"), "#", method: :delete, data: {confirm: t("you_sure")} %>
  </td>
</tr>
